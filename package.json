{
  "name": "gotron",
  "version": "1.0.0",
  "description": "Gotron is a javascript automation software for android. It allows you to manage apps, tap on the screen and more.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "manTest": "npx ts-node index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ToberoCat/gotron"
  },
  "bugs": {
    "url": "https://github.com/ToberoCat/gotron/issues"
  },
  "homepage": "https://github.com/ToberoCat/gotron#readme",
  "keywords": [
    "android",
    "automation",
    "gotron"
  ],
  "author": "Tobero",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/xmldom": "^0.1.31",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  }
}
